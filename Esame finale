   "outputs": [],
   "source": [
    "# Calcolo dei descrittori statistici per i casi totali per due continenti specificati\n",
    "def calculate_statistics(continent1, continent2):\n",
    "    total_cases_worldwide = df['total_cases'].sum()\n",
    "    \n",
    "    continent1_total_cases = df[df['continent'] == continent1]['total_cases'].sum()\n",
    "    continent1_max = df[df['continent'] == continent1]['total_cases'].max()\n",
    "    continent1_mean = df[df['continent'] == continent1]['total_cases'].mean()\n",
    "    continent1_percentage = (continent1_total_cases / total_cases_worldwide) * 100\n",
    "    \n",
    "    continent2_total_cases = df[df['continent'] == continent2]['total_cases'].sum()\n",
    "    continent2_max = df[df['continent'] == continent2]['total_cases'].max()\n",
    "    continent2_mean = df[df['continent'] == continent2]['total_cases'].mean()\n",
    "    continent2_percentage = (continent2_total_cases / total_cases_worldwide) * 100\n",
    "    \n",
    "    print(f\"\\nStatistiche per {continent1}:\")\n",
    "    print(f\"Valore massimo: {continent1_max}\")\n",
    "    print(f\"Media: {continent1_mean}\")\n",
    "    print(f\"Percentuale rispetto al numero dei casi totali nel mondo: {continent1_percentage:.2f}%\")\n",
    "    \n",
    "    print(f\"\\nStatistiche per {continent2}:\")\n",
    "    print(f\"Valore massimo: {continent2_max}\")\n",
    "    print(f\"Media: {continent2_mean}\")\n",
    "    print(f\"Percentuale rispetto al numero dei casi totali nel mondo: {continent2_percentage:.2f}%\")"
   ]
  },
  {
